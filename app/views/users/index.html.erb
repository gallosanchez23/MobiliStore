<div style="padding-top: 50px">
	<style type="text/css">
		.login-header{
	position: relative;
	height: 100%;
	background-color: #9e9e9e;
	padding-top: 50px;
}
.login-header .div-logo{
	text-align: center;
}
.login-header .logo-img{
	width: 500px;
}
.login-header .login-container{
	width: 400px;
	margin: 40px auto 10px;
	background-color: #037F6D;
	border-radius: 7px;
	padding: 10px 30px 30px;
	color: #9e9e9e;
}
.login-header .login-container .icon.left{
	margin-right: 10px;
}
.login-header .login-container form .login-input,
.login-header .login-container form h3{
	padding-bottom: 15px;
	text-align: center;
}
.login-header .login-container form div .form-control{
	background-color: #e9e9e9;
	height: 43px;
	border-radius: 3px;
}
.login-btn-section{
	padding-top: 10px;
	padding-bottom: 10px;
}
.login-btn{
	background-color: #9e9e9e;
	color: #e9e9e9;
	border-color: #9e9e9e;
}
.login-btn:hover{
	background-color: #024036;
	color: #e9e9e9;
	border-color: #23313F;
}
.back-to-home-btn{
	background-color: #e9e9e9;
	color: #999;
	border-radius: 3px;
}
	</style>
	<header class="login-header">
		<div class="div-logo"><img src="img/header_2.png" class="logo-img"></div>
		<div class="login-container">
			<form action="" class="">
				<h3><i class="icon left fa fa-unlock-alt fa-fw" aria-hidden="true"></i>Login</h3>
				<div class="login-input">
					<input type="text" class="form-control" placeholder="Username">
				</div>
				<div class="login-input">
					<input type="password" class="form-control" placeholder="Password">
				</div>
				<div class="login-btn-section">
					<a href="#" class="login-btn btn btn-default">Login</a>
					<a href="index.html" class="back-to-home-btn btn btn-default">Back to Home</a>
				</div>
			</form>
		</div>
	</header>
	<%#= User.all.inspect %>
</div>